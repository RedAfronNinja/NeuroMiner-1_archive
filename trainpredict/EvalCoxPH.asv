function [Thresh, Time] = EvalCoxPH(Models)

[ix, jx, nclass] = size(Models);
Thresh = cell(ix,jx,nclass);
Time = ExamFreq;

for h=1:nclass
    for k = 1:ix
        for l= 1:jx
            Thresh{k,l,h} = Models{k,l,h}{1}.cutoff.; 
            Time{k,l,h} = Models{k,l,h}{1}.predicted_time; 
        end
    end
end